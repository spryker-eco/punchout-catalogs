{
  "name": "spryker-eco/punchout-catalogs",
  "type": "library",
  "description": "PunchoutCatalogs module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "punchout-catalogs/punchout-catalog-spryker": "^2.0.0",
    "spryker/company": "^1.0.0",
    "spryker/company-business-unit": "^2.9.0",
    "spryker/company-user": "^2.0.0",
    "spryker/customer": "^7.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-date-time": "^1.0.0",
    "spryker/util-text": "^1.1.0",
    "spryker/vault": "^1.0.0"
  },
  "require-dev": {
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerEco\\": "src/SprykerEco/",
      "SprykerEcoTest\\Zed\\PunchoutCatalogs\\Helper\\": "tests/SprykerEcoTest/Zed/PunchoutCatalogs/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerEcoTest\\": "tests/SprykerEcoTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml -p src/",
    "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml -p src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
